    <title><%- title %> </title>
    <link rel="stylesheet" type="text/css" href="css/signup.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous" />
  </head>
  <body>
    <div class="container d-flex align-items-center justify-content-center">
      <div class="row">
        <div class="col box">
          <h4 class="fw-bold">Welcome to Traditional Games</h4>
          <h5>Create account</h5>
          <p class="text-muted">Please insert your personal information</p>
          <form method="post" action="/">
            <div class="wrapper">
              <div class="row">
                <div class="col-md-6">
                  <div class="mb-3 form">
                    <input type="text" class="form-control" id="firstName" placeholder="First name" name="firstName" required />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3 form">
                    <input type="text" class="form-control" id="lastName" placeholder="Last Name" name="lastName" required />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6">
                  <div class="mb-3 form">
                    <input type="email" class="form-control" id="email" placeholder="Email address" name="email" required />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3 form">
                    <input type="password" class="form-control" id="password" placeholder="Password" name="password" required />
                  </div>
                </div>
              </div>
              <button type="submit" class="btn btn-primary w-100" style="border-radius: 10px">Submit</button>
            </div>
            <div class="to-login d-flex align-items-center justify-content-center mt-3">
              <p class="text-muted mb-0 me-2">Already have an account?</p>
              <a href="#" class="fw-bold text-decoration-none">Sign In</a>
            </div>
          </form>
        </div>
      </div>
    </div>

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

